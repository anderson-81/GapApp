<?xml version='1.0' encoding='utf-8'?>
<widget id="com.anderson.gapapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>GapApp</name>
    <description>Android application using jQuery Mobile that seeks to show the basic form of the use of standards PhoneGap plugins.</description>
    <author email="andersonprofissional81@gmail.com" href="http://testando.orgfree.com">Anderson [Developer]</author>
    <content src="index.html" />
    <icon src="img/logo-bra.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
